      "cell_type": "code",
      "source": [
        "voting_clf_hard = Pipeline(steps=[\n",
        "    ('preprocessor', preprocessor),\n",
        "    ('classifier', VotingClassifier(estimators=[\n",
        "    ('dt', dt), ('rf', rf), ('gb', gb),('lg',lg),(\"kn\",kn) ],\n",
        "      voting='hard'))#magorty voting\n",
        "])\n",
        "\n"
      ],
